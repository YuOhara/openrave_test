<launch>
  <node pkg="jsk_pcl_ros" type="kinfu" name="kinfu" output="screen">
    <remap from="~input/depth" to="/camera_remote/depth_registered/hw_registered/image_rect_raw_calibrated" />
    <remap from="~input/color" to="/camera_remote/rgb_half/camera_info" />
    <remap from="~input/info" to="/camera_remote/depth_registered/points" />
    <rosparam>
      tsdf_trunc_dist: 0.03
      icp_dist_trans: 0.1
      icp_dist_rot: 20
      camera_movement_thre: 0.001
      child_frame_id: /camera_depth_optical_frame
    </rosparam>
  </node>
</launch>
